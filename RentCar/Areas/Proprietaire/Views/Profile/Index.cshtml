@model RentCar.Models.Proprietaire

@{
    ViewData["Title"] = "Index";
}
<a asp-action="listVoiture" asp-route-id="@Model.Id" class="btn btn-info top-right" />Liste des voitures </a>
<h2 class="text text-info text-capitalize text-muted">Profile</h2>

<hr />
<div class=" row table-bordered " style="padding:15px 15px 15px 15px">
    <div class="col-md-8">
        <form asp-action="Index">
            <div class="form-group row">
                <div class="col-md-4">
                    <label asp-for="Id" class="control-label"></label>
                </div>
                <div class="col-md-8">
                    <input asp-for="Id" readonly class="form-control" />
                </div>

            </div>
            <div class="form-group row">
                <div class="col-md-4">
                    <label asp-for="Name" class="control-label"></label>
                </div>
                <div class="col-md-8">
                    <input asp-for="Name" readonly class="form-control" />
                </div>

            </div>
            <div class="form-group row">
                <div class="col-md-4">
                    <label asp-for="adresse" class="control-label"></label>
                </div>
                <div class="col-md-8">
                    <input asp-for="adresse" readonly class="form-control" />
                </div>
            </div>
            <div class="form-group">

                @Html.ActionLink("Change", "Edit", new { id = Model.Id })

            </div>
        </form>
    </div>
</div>




